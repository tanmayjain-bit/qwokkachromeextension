{
    "manifest_version": 3,
    "name": "Qwokka",
    "version": "1.0",
    "description": "See what's actually good on Netflix.",
    "icons": {
      "48": "icon.png"
    },
    "permissions": [
      "https://ajax.googleapis.com/"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.netflix.com/*"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }
  